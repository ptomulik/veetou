CREATE TEMP VIEW table_decl_strings AS
  SELECT
    table_name, ('
    INTO TABLE veetou_' || table_name || '
    WHEN tab_id = ''' || table_id || '''
    FIELDS TERMINATED BY ";" OPTIONALLY ENCLOSED BY ''"''
    TRAILING NULLCOLS ' ||
    field_list)
  AS decl
  FROM table_decls
;

