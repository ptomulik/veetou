--
-- TABLE DATA
--


.mode csv
.header off
.separator ";" "\n"

----------- ko_jobs
SELECT
    (SELECT table_id FROM table_decls WHERE table_name = 'ko_jobs') AS table_id
  , (SELECT value FROM parse_uuid) AS parse_uuid
  , (SELECT value FROM ko_variables WHERE name = 'parse_timestamp') AS parse_timestamp
  , (SELECT value FROM ko_variables WHERE name = 'parse_host') AS parse_host
  , (SELECT value FROM ko_variables WHERE name = 'parse_user') AS parse_user
  , (SELECT value FROM ko_variables WHERE name = 'parse_name') AS parse_name
;

----------- ko_footers
